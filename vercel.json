{
  "rewrites": [
    {
      "source": "/api/meetingData/topicList.json",
      "destination": "/api/meetingData/topicListApi"
    },
    {
      "source": "/api/meetingData/topicDetail.json",
      "destination": "/api/meetingData/topicDetailApi"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
